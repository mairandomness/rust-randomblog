{% extends "main" %}
{% block content %}
    <div class="container">
        <h2>{{ post.title }}</h2>
        <p class="date">{{ post.date }}</p>
        <p>{{ post.content }}</p>
    </div>
    <div class="container">
        <a href="{{ PATH }}/post/{{ post.id - 1 }}">  Previous Post </a>
        <a class="right-align" href="{{ PATH }}/post/{{ post.id + 1 }}"> Next Post </a>
    </div>
{% endblock content %}